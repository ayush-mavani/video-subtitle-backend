{
    "version": 2,
    "public": true,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/public/:path*",
            "destination": "/:path*"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        },
        {
            "src": "./public",
            "dest": "./index.js"
        }
    ]
}