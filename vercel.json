{
    "version": 2,
    "public": true,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        },
        {
            "src": "./public",
            "dest": "./index.js"
        }
    ]
}